import time
from math import sqrt
start = time.time()
def is_prime(n):
    """function to check if the given
    number is prime or not"""
    if n % 2 == 0:
        return False
    else:
        for i in xrange(3, int(n**0.5+1),2):
            if n % i == 0:
                return False
        return True
number = 3
primes = [2]
while True:
    if is_prime(number):
        primes.append(number)
    else:
        for i in primes:
            if math.sqrt(((number-i)/2)) == int(math.sqrt(((number-i)/2))):
                break
        else:
            print number
            break

    number += 2
end = time.time()
print end - start
